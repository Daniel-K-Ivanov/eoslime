{
    "template": "nodeos [port] [accounts]",
    "description": "Run local nodeos",
    "options": [
        {
            "name": "port",
            "definition": {
                "describe": "Port to run nodeos on",
                "type": "number",
                "default": 8888
            }
        },
        {
            "name": "with-example",
            "definition": {
                "describe": "Provides eosio.token contract and tests as an example for usage",
                "type": "boolean",
                "default": false
            }
        }
    ]
}